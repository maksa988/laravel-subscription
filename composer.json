{
    "name": "maksa988/laravel-subscription",
    "description": "Laravel Subscription provides an expressive, fluent interface to subscription billing services.",
    "keywords": ["laravel", "subscription", "payments", "billing"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/maksa988/laravel-subscription/issues",
        "source": "https://github.com/maksa988/laravel-subscription"
    },
    "authors": [
        {
            "name": "maksa988",
            "email": "maksa988ua@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Maksa988\\LaravelSubscription\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maksa988\\LaravelSubscription\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maksa988\\LaravelSubscription\\LaravelSubscriptionServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
